{
  "problemMatcher": [
    {
      "owner": "dotnet-format",
      "pattern": [
        {
          "regexp": "^\\s*Formatting code files in workspace '(.*)'\\.$",
          "fromPath": 1
        },
        {
          "regexp": "^\\s+(.*)\\((\\d+),(\\d+)\\):\\s+(.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "message": 4,
          "loop": true
        }
      ]
    }
  ]
}
